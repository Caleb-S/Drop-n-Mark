{
    "sourceRoot": "src/components",
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "chrome": "67"
          },
          "useBuiltIns": false,
          "forceAllTransforms": true
        }
      ]
    ],
    "minified": true,
     "only": ["src/components"],
     "ignore": ["src/components/dist", "src/components/styles"],
    "assumptions": {
        "superIsCallableConstructor": true
      }
  }
